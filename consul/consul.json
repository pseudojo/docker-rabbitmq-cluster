{
    "server": true,
    "datacenter": "dc1",
    "domain": "consul",
    "data_dir": "/opt/bitnami/consul/data",
    "pid_file": "/opt/bitnami/consul/tmp/consul.pid",
    "ui": true,
    "bootstrap": true,
    "bootstrap_expect": 0,
    "bind_addr": "0.0.0.0",
    "serf_lan": "0.0.0.0",
    "addresses": {
        "http": "0.0.0.0",
        "grpc": "0.0.0.0",
        "dns": "0.0.0.0"
    },
    "retry_join": [
        "0.0.0.0"
    ],
    "ports": {
        "server": 8300,
        "serf_lan": 8301,
        "http": 8500,
        "grpc": 8502,
        "dns": 8600
    },
    "log_level": "INFO",
    "telemetry": {
        "prometheus_retention_time": "60s",
        "disable_hostname": true
    },
    "enable_central_service_config": true,
    "enable_local_script_checks": true
}
